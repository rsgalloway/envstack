{
    "author": "ryan@rsg.io",
    "targets": {
        "env": {
            "source": "env/*",
            "destination": "{DEPLOY_ROOT}/env/%1"
        },
        "bin": {
            "source": "bin/*",
            "destination": "{DEPLOY_ROOT}/bin/%1"
        },
        "bash_completion": {
            "source": "bin/envstack_completion.sh",
            "destination": "/etc/bash_completion.d/envstack"
        },
        "lib": {
            "source": "lib/envstack",
            "destination": "{DEPLOY_ROOT}/lib/python/envstack"
        }
    }
}