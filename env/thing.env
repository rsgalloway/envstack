#!/usr/bin/env envstack

include: [default]

all: &default
  HELLO: goodbye
  LOG_LEVEL: ${LOG_LEVEL:=INFO}
  FLOAT: 1.0
  INT: 5
  NUMBER_LIST: [1, 2, 3]
  CHAR_LIST: ['a', 'b', 'c', "${HELLO}"]
  DICT: {a: 1, b: 2, c: 3}

darwin:
  <<: *default

linux:
  <<: *default
  ROOT: /var/tmp/pipe

windows:
  <<: *default
